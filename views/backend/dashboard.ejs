                        <!-- Orders -->
                        <% if(adminType == "Publication"){ %>
                        <section class="orders light-section">
                            <div class="mx-auto pt-4">
                                <h3 class="font-weight-bold mb-3">Book Sold History (<%= admindata.firstname %> <%= admindata.lastname %>)</h3>
                        
                          
                                <table class="" style="width: 100%;background-color: white;text-align: center !important;">
                                    <thead>
                                        <tr>
                                            <th class="px-4 py-3 text-center">S.N</th>
                                            <th class="px-4 py-3 text-center">Book Name</th>
                                            <th class="px-4 py-3 text-center">Book Sold (Quantity)</th>
                                
                                            <!-- <th class="px-4 py-3 text-center">Status</th> -->
                                        </tr>
                                    </thead>
                                    <tbody >
                                        <% if(publicationWiseBookSold.length > 0){ %>
                                            <% publicationWiseBookSold.forEach(function(data,index){ %>
                                                <tr>
                                                    <td class="border px-2 py-3"><%= index + 1 %></td>
                                                    <td class="border px-2 py-3"><span style="background: #6b5ddeeb;
                                                        color: white;
                                                        padding: 6px 13px;"><%= data.book_name %></span></td>
                                                    <td class="border px-2 py-3 font-weight-bold"><%= data.book_sold %> pcs</td>
                                                </tr>

                                          <%  }) %>
                                        <% } %>
              
                                    </tbody>
                                </table>
                            </div>
                        </section>
                        <% } %>



                        <% if(adminType == "Sub Admin" || adminType == "Super Admin"){ %>
                        <section class="orders light-section">
                            <div class="mx-auto pt-4">
                                <h3 class="font-weight-bold mb-3">All Orders</h3>
                        
                          
                                <table class="" style="width: 100%;background-color: white;text-align: center !important;">
