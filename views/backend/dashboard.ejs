                                            <th class="px-4 py-3 text-center">Customer</th>
                                            <th class="px-4 py-3 text-center">Contact</th>
                                            <th class="px-4 py-3 text-center">Address</th>
                                            <th class="px-2 py-3 text-center">Payment Method</th>
                                            <th class="px-4 py-3 text-center">Status</th>
                                            <th class="px-4 py-3 text-center">Placed at</th>
                                            <!-- <th class="px-4 py-3 text-center">Status</th> -->
                                        </tr>
                                    </thead>
                                    <tbody id="orderTableBody">
                        
                                    </tbody>
                                </table>
                            </div>
                        </section>
