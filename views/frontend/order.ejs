 <section class="section" style="width: 100%;">
    <div class="container blogs">
        <h3 class="font-weight-bold mb-4">All Orders</h3>
      <div class="row">
            <table style="width: 100%;background-color: whitesmoke;">
                <thead>
                    <tr>
                        <th class="py-3 px-4 text-left">Your Order Id</th>
                        <th class="py-3 px-4 text-left">FullName</th>
                        <th class="py-3 px-4 text-left">Address</th>
                        <th class="py-3 px-4 text-left">Phone Number</th>
                        <th class="py-3 px-4 text-left">Time</th>
                    </tr>
                </thead>
                <tbody>
                    <script src="../frontend_js/jquery.js"></script>
                    <% if(orderData.length > 0){  %>
                        <% orderData.forEach(function(data,index){%>
                            <% if(data.status == 'completed'){ %>
                                
                                <tr style="background-color:#fdfdfd">  
                                   <td class="border py-3 px-4"> <s><a class="link font-weight-bold" href="/orders/<%= data._id %>" style="font-size: 16px;" id="order_id"><%= index + 1 %> ) &nbsp;&nbsp;<%= data.orderId %></a></s></td>
                                   <td class="border py-3 px-4"> <s><%= data.fullName %></s></td>
                                   <td class="border py-3 px-4"> <s><%= data.city %>, <%= data.streetAddress %></s></td>
                                    <td class="border py-3 px-4"> <s><%= data.phoneNumber %></s></td>
                                    <td class="border py-3 px-4"> <s> <%= moment(data.createdAt).format('MMM Do hh:mm A') %></s></td>
                                </tr>
                            <% }else{ %>
                                <tr style="background-color:#fdfdfd">  
                                    <td class="border py-3 px-4"><a class="link font-weight-bold" href="/orders/<%= data._id %>" style="font-size: 16px;" id="order_id"><%= index + 1%> ) &nbsp;&nbsp;<%= data.orderId  %></a></td>
                                    <td class="border py-3 px-4"> <%= data.fullName %></td>
                                    <td class="border py-3 px-4"><%= data.city %>, <%= data.streetAddress %></td>
                                    <td class="border py-3 px-4"><%= data.phoneNumber %></td>
                                    <td class="border py-3 px-4">   <%= moment(data.createdAt).format('MMM Do hh:mm A') %></td>
                                </tr>
                                <% } %>
                    <%  })  %>
                    <% }else{ %>
                        <td class="px-4 py-3">Orders not found</td>
                    <% } %>
                </tbody>
            </table>
      </div>
    </div> 
   </section>




   <%- include('../layouts/frontend/footer') %>
