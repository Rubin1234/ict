function imageValidation(){

   var validExt = ["jpeg","png","jpg"];
   var imageInput = $(event.currentTarget).val();


   var pos_of_dot = imageInput.lastIndexOf('.')+1;
   var img_ext = imageInput.substring(pos_of_dot);
   var result = validExt.includes(img_ext);

   if(imageInput != ''){
      if(result == false){
         $('#imgError').empty().append(' <p style="color: #f1556c;margin-bottom: 10px;"><i class="mdi mdi-close-circle"></i> Image File is not Image</p>')
         event.preventDefault();
         return false;
      }else{
         $('#imgError').empty();
      }
      return true;
   }
}



function pdfValidation(){
   var validExt = ["pdf"];
   var pdfInput = $(event.currentTarget).val();


   var pos_of_dot = pdfInput.lastIndexOf('.')+1;
   var pdf_ext = pdfInput.substring(pos_of_dot);
   var result = validExt.includes(pdf_ext);

   if(pdfInput != ''){
      if(result == false){
         $(event.currentTarget).siblings('#pdfError').empty().append(' <p style="color: #f1556c;margin-bottom: 10px;"><i class="mdi mdi-close-circle"></i>  File is not in PDF Format</p>')
         event.preventDefault();
         return false;
      }else{
         $(event.currentTarget).siblings('#pdfError').empty();
      }
      return true;
   }
}

            
function multipleImageFormValidation(){
   var validExt = ["jpeg","png","jpg"];
   $('#imgError').empty();


   var files = document.forms['multipleImageForm']['product_images'].files;

   if (files.length > 0) {
   for (var i = 0; i < files.length; i++)
      {
         var file = file = files[i];
         var filename = file.name;

         var pos_of_dot = filename.lastIndexOf('.')+1;
         var image_extension = filename.substring(pos_of_dot);
         var result = validExt.includes(image_extension);

         if(result == false){
            $('#imgError').append(' <p style="color: #f1556c;margin-bottom: 10px;"><i class="mdi mdi-close-circle"></i>'+filename+'   File is not in image format</p>')
            event.preventDefault(); 
         }
      }
 
}else{

            $('#imgError').append(' <p style="color: #f1556c;margin-bottom: 10px;"><i class="mdi mdi-close-circle"></i>Image is required.</p>')
            event.preventDefault(); 
     
}

}


